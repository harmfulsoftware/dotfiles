[alias]
  a = add -A
  ad = add -A --dry-run
  b = branch -vv
  c = "!git switch master; git fetch -p && git branch -vv | grep ': gone]' | awk '{print $1}' | xargs git branch -D; git pull origin $(git rev-parse --abbrev-ref HEAD) --no-tags"
  d = diff
  f = "!git add -A; git commit --allow-empty-message; git push"
  g = "!git gc; git repack -Ad; git prune"
  p = "!git pull origin $(git rev-parse --abbrev-ref HEAD) --no-tags && git submodule update --remote"
  pu = "!git push -u origin $(git rev-parse --abbrev-ref HEAD)"
  s = switch
  sc = switch -c
[core]
  compression = 0
[credential]
  helper =
[http]
  sslCipherList = "ECDHE-RSA-CHACHA20-POLY1305"
  sslVersion = tlsv1.2
[pack]
  useSparse = true
[pull]
  rebase = true
[rebase]
  autoStash = true
[transfer]
  fsckobjects = true
[user]
  useConfigOnly = true
